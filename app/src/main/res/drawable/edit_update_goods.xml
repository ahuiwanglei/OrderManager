<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape>
            <solid android:color="@android:color/transparent" />
            <stroke android:width="1dip" android:color="@color/default_bar_bg" android:dashGap="0dp" android:dashWidth="0dip" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item android:state_focused="false">
        <shape>
            <solid android:color="@android:color/transparent" />
            <stroke android:width="1dip" android:color="@color/default_bar_bg_select" android:dashGap="0dp" android:dashWidth="0dip" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@android:color/transparent" />
            <stroke android:width="1dip" android:color="@color/default_bar_bg_select" android:dashGap="0dp" android:dashWidth="0dip" />
            <corners android:radius="5dp" />
        </shape>
    </item>
</selector>